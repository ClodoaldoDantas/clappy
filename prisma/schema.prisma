generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Room {
  id          String   @id @default(cuid())
  name        String
  description String
  createdAt   DateTime @default(now()) @map("created_at")

  @@map("rooms")
}
